formData.js


function signup(){
    event.preventDefault();

    /*let uid=document.getElementById("uid").value;
    let uname=document.getElementById("uname").value;

    let pwd=document.getElementById("pwd").value;
    let cpwd=document.getElementById("cpwd").value;

    let mobile=document.getElementById("mobile").value;
    let email=document.getElementById("email").value;

    let dob=document.getElementById("dob").value;*/

    //let gender=document.getElementById("gen").value;

    //let gender=document.querySelector('input[name="gen"]:checked');

    //console.log(gender.value);

    //console.log(uid+ "\n"+ uname + "\n" + pwd + "\n" + cpwd + "\n" + mobile + "\n" + email + "\n" +dob + "\n" + gender);


    //let skills=document.querySelectorAll('input[name="skill"]:checked');
  

    /*skills.forEach(element =>{
        console.log(element.value);
    });*/

    let degree=document.getElementById("qual").value;
    console.log(degree);

    let food=document.getElementById("like").value;
    console.log(food);

    let addr=document.getElementById("addr").value;
    console.log(addr);

    //let photo=document.getElementById("photo").value;
    //console.log(photo);

    //let resume=document.getElementById("resume").value;
    //console.log(resume);
}









